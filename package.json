    "@types/jest": "^30.0.0",
    "@types/node": "^20.12.7",
    "jest": "^30.2.0",
    "semantic-release": "^25.0.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
