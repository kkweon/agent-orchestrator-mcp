{
  "name": "@kkweon/agent-orchestrator-mcp",
  "version": "1.1.4",
  "description": "Agent Orchestrator MCP Server for tmux-based sub-agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkweon/agent-orchestrator-mcp.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
